<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox prefHeight="1000.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eli.projects.spprototype.vista.PiecePresenter">
   <children>
      <Button mnemonicParsing="false" onAction="#exportPiece" styleClass="large_button" stylesheets="@../style.css" text="Export">
         <graphic>
            <FontIcon iconLiteral="enty-export" />
         </graphic>
      </Button>
      <TableView fx:id="partTable" editable="true" prefHeight="1000.0" prefWidth="1000.0">
         <columns>
            <TableColumn fx:id="partDesignationColumn" editable="false" prefWidth="75.0" text="Part" />
            <TableColumn fx:id="partPageCountColumn" editable="false" prefWidth="75.0" text="Pages" />
         </columns>
      </TableView>
   </children>
</VBox>
