                              </Label>
                              <Label fx:id="labelPieces1" styleClass="category_option2" text="Random Piece">
                                 <graphic>
                                    <FontIcon iconLiteral="enty-shuffle" wrappingWidth="20.0" />
                                 </graphic>
                              </Label>
                           </children>
                           <VBox.margin>
                              <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
                           </VBox.margin>
                        </VBox>
                        <Label styleClass="category_header2" text="Recent Pieces" />
                        <ListView prefHeight="200.0" prefWidth="200.0" />
                        <Label layoutX="10.0" layoutY="176.0" styleClass="category_header2" text="Ensembles" />
                        <ListView layoutX="10.0" layoutY="202.0" prefHeight="200.0" prefWidth="200.0" />
                        <Label layoutX="10.0" layoutY="402.0" styleClass="category_header2" text="Lists" />
                        <ListView layoutX="10.0" layoutY="428.0" prefHeight="200.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </children>
               <opaqueInsets>
                  <Insets />
